// Woeful_Wolf

struct tpGxTexData(tpGxTexHead &texHead) {
	local uint32 fileDataStart = FTell();
    local int i;
    for (i = 0; i < texHead.header.subresourcesCount; i++) {
        FSeek(fileDataStart + texHead.subresources[i].offset);
        struct (tpGxTexHead &texHead) {
            local uint32 subresourceStart = FTell();
            FSeek(subresourceStart + texHead.subresources[i].sliceSize * texHead.subresources[i].depth);
        } subresource(texHead);
    }
};